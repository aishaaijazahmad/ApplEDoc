@prefix : <http://www.kracr.iiitd.edu.in/ontology/apple#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix AIRO: <https://w3id.org/AIRO#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix swrl: <http://www.w3.org/2003/11/swrl#> .
@prefix time: <http://www.w3.org/2006/time#> .
@prefix apple: <http://www.kracr.iiitd.edu.in/ontology/apple/> .
@prefix event: <http://w3id.org/daselab/onto/event#> .
@prefix swrlb: <http://www.w3.org/2003/11/swrlb#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix traffic: <http://www.sensormeasurement.appspot.com/ont/transport/traffic#> .
@prefix objectrole: <http://www.ontologydesignpatterns.org/cp/owl/objectrole.owl#> .
@prefix trajectory: <http://w3id.org/daselab/onto/trajectory#> .
@prefix participation: <http://www.ontologydesignpatterns.org/cp/owl/participation.owl#> .
@prefix coparticipation: <http://www.ontologydesignpatterns.org/cp/owl/coparticipation.owl#> .
@base <http://www.kracr.iiitd.edu.in/ontology/apple> .

<http://www.kracr.iiitd.edu.in/ontology/apple> rdf:type owl:Ontology ;
                                                owl:versionIRI <http://www.kracr.iiitd.edu.in/ontology/apple/1.0> ;
                                                terms:contributor "Manohar Kumar"^^xsd:string ;
                                                terms:creator "Aisha Aijaz"^^xsd:string ,
                                                              "Raghava Mutharaju"^^xsd:string ;
                                                terms:description "An Ontology for applied ethics and context to capture relevant ethical information that may provide insight to the morality of a course of action during a particular event."^^xsd:string ;
                                                terms:title "ApplE: An Applied Ethics Ontology"^^xsd:string ;
                                                owl:versionInfo "Version 1.0 of the ApplE Ontology for Applied Ethics."^^xsd:string .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/terms/contributor
terms:contributor rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/creator
terms:creator rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/description
terms:description rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/title
terms:title rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://w3id.org/daselab/onto/event#hasInformationObject
event:hasInformationObject rdf:type owl:ObjectProperty ;
                           owl:inverseOf apple:occursInEvent ;
                           rdfs:domain <http://schema.org/Event> ;
                           rdfs:range <http://schema.mobivoc.org/Action> ;
                           rdfs:comment """Property relating Event to InformationObject in this pattern. The domain of this property is not restricted to Event  since it is possible that non-event to have an information object. The range is always InformationObject since it makes no sense to have a property named hasInformationObject to point to anything other than information object.

However, when reusing this concept from the EventCore ODP for the ApplE ontology, this property will link an event and an action.""" .


###  http://w3id.org/daselab/onto/event#subEventOf
event:subEventOf rdf:type owl:ObjectProperty ;
                 rdfs:domain <http://schema.org/Event> ;
                 rdfs:range <http://schema.org/Event> ;
                 rdfs:comment "Property expressing partonomic relation between two events. Domain and range are always Event." .


###  http://w3id.org/daselab/onto/trajectory#atPlace
trajectory:atPlace rdf:type owl:ObjectProperty ,
                            owl:AsymmetricProperty ,
                            owl:IrreflexiveProperty ;
                   rdfs:domain <http://schema.org/Event> ;
                   rdfs:range trajectory:Place .


###  http://w3id.org/daselab/onto/trajectory#atTime
trajectory:atTime rdf:type owl:ObjectProperty ,
                           owl:AsymmetricProperty ,
                           owl:IrreflexiveProperty ;
                  rdfs:domain <http://schema.org/Event> ;
                  rdfs:range trajectory:TimeEntity .


###  http://www.kracr.iiitd.edu.in/ontology/apple#hasDurationOfConsequence
:hasDurationOfConsequence rdf:type owl:ObjectProperty ;
                          rdfs:subPropertyOf apple:hasCharacteristicOfConsequence .


###  http://www.kracr.iiitd.edu.in/ontology/apple#hasSeverityOfConsequence
:hasSeverityOfConsequence rdf:type owl:ObjectProperty ;
                          rdfs:subPropertyOf apple:hasCharacteristicOfConsequence .


###  http://www.kracr.iiitd.edu.in/ontology/apple#hasUtilityOfConsequence
:hasUtilityOfConsequence rdf:type owl:ObjectProperty ;
                         rdfs:subPropertyOf apple:hasCharacteristicOfConsequence .


###  http://www.kracr.iiitd.edu.in/ontology/apple#violatedByAction
:violatedByAction rdf:type owl:ObjectProperty ;
                  owl:inverseOf apple:violatesEthicalPrinciple .


###  http://www.kracr.iiitd.edu.in/ontology/apple/adheresTo
apple:adheresTo rdf:type owl:ObjectProperty ;
                owl:inverseOf apple:appliesTo ;
                rdf:type owl:FunctionalProperty ,
                         owl:AsymmetricProperty ,
                         owl:IrreflexiveProperty ;
                rdfs:domain AIRO:Domain ;
                rdfs:range apple:AppliedEthics ;
                rdfs:comment "A domain adheres to a specific type of applied ethics. For example, the medical domain adheres to bioethics. Again, this connection is not absolute. For example, an issue of the medical domain may also require adherance to the professional domain, when considering the treatment of nurses and doctors in large private hospitals." .


###  http://www.kracr.iiitd.edu.in/ontology/apple/affects
apple:affects rdf:type owl:ObjectProperty ,
                       owl:AsymmetricProperty ,
                       owl:IrreflexiveProperty ;
              rdfs:domain <http://schema.mobivoc.org/Action> ;
              rdfs:range apple:PassiveAgent ;
              rdfs:comment "Affects implies the verb in the statement \"an action affects a passive agent\" in whatever way, shape, or form." .


###  http://www.kracr.iiitd.edu.in/ontology/apple/appliesPhilosophy
apple:appliesPhilosophy rdf:type owl:ObjectProperty ,
                                 owl:AsymmetricProperty ,
                                 owl:IrreflexiveProperty ;
                        rdfs:domain apple:AppliedEthics ;
                        rdfs:range apple:AppliedEthicsPhilosophy ;
                        rdfs:comment "A type of applied ethics may apply a type of philosophy." .


###  http://www.kracr.iiitd.edu.in/ontology/apple/appliesTo
apple:appliesTo rdf:type owl:ObjectProperty ;
                rdfs:comment "Inverse of the adheresTo object property. This property implies that a type of applied ethics applies to a certain domain." .


###  http://www.kracr.iiitd.edu.in/ontology/apple/associatedWith
apple:associatedWith rdf:type owl:ObjectProperty ,
                              owl:AsymmetricProperty ,
                              owl:IrreflexiveProperty ;
                     rdfs:domain apple:NormativeEthics ;
                     rdfs:comment "Types of normative ethics may be associated with certain ethical concepts." .


###  http://www.kracr.iiitd.edu.in/ontology/apple/causesEthicalIssue
apple:causesEthicalIssue rdf:type owl:ObjectProperty ,
                                  owl:AsymmetricProperty ,
                                  owl:IrreflexiveProperty ;
                         rdfs:domain AIRO:Consequence ;
                         rdfs:range apple:EthicalIssue ;
                         rdfs:comment "The consequence of an action may cause an ethical issue. (This is not to be confused with the fact that some may consider a consequence to be an ethical issue, as these are two separate concepts. For example: the firing of an employee on the basis of their race is a consequence of the 'firing' action. However, the firing is not the ethical issue. The issue is a business ethical issue: Discrimination in the workplace.)" .


###  http://www.kracr.iiitd.edu.in/ontology/apple/describesEvent
apple:describesEvent rdf:type owl:ObjectProperty ,
                              owl:AsymmetricProperty ,
                              owl:IrreflexiveProperty ;
                     rdfs:domain <http://contextus.net/ontology/ontomedia/core/expression#Context> ;
                     rdfs:range <http://schema.org/Event> ;
                     rdfs:comment "The context describes an event that has occured." .


###  http://www.kracr.iiitd.edu.in/ontology/apple/hasCharacteristicOfConsequence
apple:hasCharacteristicOfConsequence rdf:type owl:ObjectProperty ,
                                              owl:AsymmetricProperty ,
                                              owl:IrreflexiveProperty ;
                                     rdfs:domain AIRO:Consequence ;
                                     rdfs:range apple:CharacteristicOfConsequence ;
                                     rdfs:comment "This object property describes the characteristic of the consequences of an action." .


###  http://www.kracr.iiitd.edu.in/ontology/apple/hasMoralIntention
apple:hasMoralIntention rdf:type owl:ObjectProperty ,
                                 owl:AsymmetricProperty ,
                                 owl:IrreflexiveProperty ;
                        rdfs:domain apple:ActiveAgent ;
                        rdfs:range apple:MoralIntention ;
                        rdfs:comment "An agent, when doing an action, has moral intention behind the action." .


###  http://www.kracr.iiitd.edu.in/ontology/apple/occursInDomain
apple:occursInDomain rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf owl:topObjectProperty ;
                     rdf:type owl:FunctionalProperty ,
                              owl:AsymmetricProperty ,
                              owl:IrreflexiveProperty ;
                     rdfs:domain <http://schema.org/Event> ;
                     rdfs:range AIRO:Domain ;
                     rdfs:comment "An event occurs in some domain." .


###  http://www.kracr.iiitd.edu.in/ontology/apple/occursInEvent
apple:occursInEvent rdf:type owl:ObjectProperty ,
                             owl:AsymmetricProperty ,
                             owl:IrreflexiveProperty ;
                    rdfs:domain <http://schema.mobivoc.org/Action> ;
                    rdfs:range <http://schema.org/Event> ;
                    rdfs:comment "An action occurs during some event." .


###  http://www.kracr.iiitd.edu.in/ontology/apple/resolvedBy
apple:resolvedBy rdf:type owl:ObjectProperty ;
                 owl:inverseOf apple:resolves ;
                 rdf:type owl:AsymmetricProperty ,
                          owl:IrreflexiveProperty ;
                 rdfs:domain apple:EthicalIssue ;
                 rdfs:range apple:AppliedEthicsPhilosophy ;
                 rdfs:comment "An ethical issue is resolved by an applied ethics philosophy. Here 'resolves' implies the resolution of the issue." .


###  http://www.kracr.iiitd.edu.in/ontology/apple/resolves
apple:resolves rdf:type owl:ObjectProperty ;
               rdfs:comment "Inverse of the resolvedBy property. An applied ethics philosophy resolves an applied ethics issue. Although the term is assertive (resolves), the property demostrates that a certain property may be used to resolve an ethical issue." .


###  http://www.kracr.iiitd.edu.in/ontology/apple/upholdsEthicalPrinciple
apple:upholdsEthicalPrinciple rdf:type owl:ObjectProperty ;
                              rdfs:subPropertyOf owl:topObjectProperty ;
                              rdf:type owl:AsymmetricProperty ,
                                       owl:IrreflexiveProperty ;
                              rdfs:domain <http://schema.mobivoc.org/Action> ;
                              rdfs:range apple:EthicalPrinciple ;
                              rdfs:comment "An action may uphold certain ethical principles." .


###  http://www.kracr.iiitd.edu.in/ontology/apple/violatesEthicalPrinciple
apple:violatesEthicalPrinciple rdf:type owl:ObjectProperty ,
                                        owl:AsymmetricProperty ,
                                        owl:IrreflexiveProperty ;
                               rdfs:domain <http://schema.mobivoc.org/Action> ;
                               rdfs:range apple:EthicalPrinciple ;
                               rdfs:comment "An action may violate certain ethical principles." .


###  http://www.ontologydesignpatterns.org/cp/owl/coparticipation.owl#coparticipatesWith
coparticipation:coparticipatesWith rdf:type owl:ObjectProperty ,
                                            owl:SymmetricProperty ;
                                   rdfs:domain <http://xmlns.com/foaf/0.1/Agent> ;
                                   rdfs:range <http://xmlns.com/foaf/0.1/Agent> ;
                                   rdfs:comment "Agents may co-participate with each other in an event in any capacity (active or passive)." ;
                                   rdfs:isDefinedBy "http://www.ontologydesignpatterns.org/cp/owl/coparticipation.owl#" .


###  http://www.ontologydesignpatterns.org/cp/owl/objectrole.owl#hasRole
objectrole:hasRole rdf:type owl:ObjectProperty ;
                   owl:inverseOf objectrole:isRoleOf ;
                   rdfs:domain <http://xmlns.com/foaf/0.1/Agent> ;
                   rdfs:range objectrole:Role ;
                   rdfs:comment "A relation between an object and a role, e.g. the person 'John' has role 'student'." ;
                   rdfs:isDefinedBy "http://www.ontologydesignpatterns.org/cp/owl/objectrole.owl" .


###  http://www.ontologydesignpatterns.org/cp/owl/objectrole.owl#isRoleOf
objectrole:isRoleOf rdf:type owl:ObjectProperty ;
                    rdfs:domain objectrole:Role ;
                    rdfs:range <http://xmlns.com/foaf/0.1/Agent> ;
                    rdfs:comment "A relation between an object and a role, e.g. the 'student' is the role of 'John'." ;
                    rdfs:isDefinedBy "http://www.ontologydesignpatterns.org/cp/owl/objectrole.owl" .


###  http://www.ontologydesignpatterns.org/cp/owl/participation.owl#hasParticipant
participation:hasParticipant rdf:type owl:ObjectProperty ;
                             owl:inverseOf participation:isParticipantIn ;
                             rdf:type owl:AsymmetricProperty ,
                                      owl:IrreflexiveProperty ;
                             rdfs:domain <http://schema.org/Event> ;
                             rdfs:range <http://xmlns.com/foaf/0.1/Agent> ;
                             rdfs:comment "Inverse of the \"isParticipantIn\" object property from the participant Ontology Design Pattern. Shows an event which has a participating agent." ;
                             rdfs:isDefinedBy "http://www.ontologydesignpatterns.org/cp/owl/participation.owl#" .


###  http://www.ontologydesignpatterns.org/cp/owl/participation.owl#isParticipantIn
participation:isParticipantIn rdf:type owl:ObjectProperty ,
                                       owl:AsymmetricProperty ,
                                       owl:IrreflexiveProperty ;
                              rdfs:domain <http://xmlns.com/foaf/0.1/Agent> ;
                              rdfs:range <http://schema.org/Event> ;
                              rdfs:comment "An agent is a participant in an event." ;
                              rdfs:isDefinedBy "http://www.ontologydesignpatterns.org/cp/owl/participation.owl#" .


###  http://www.sensormeasurement.appspot.com/ont/transport/traffic#doesAction
traffic:doesAction rdf:type owl:ObjectProperty ,
                            owl:AsymmetricProperty ,
                            owl:IrreflexiveProperty ;
                   rdfs:domain apple:ActiveAgent ;
                   rdfs:range <http://schema.mobivoc.org/Action> ;
                   rdfs:comment """Doing an action. 
This property describes the relationship between an active agent and the action they do.""" .


###  https://w3id.org/AIRO#hasConsequence
AIRO:hasConsequence rdf:type owl:ObjectProperty ,
                             owl:AsymmetricProperty ,
                             owl:IrreflexiveProperty ;
                    rdfs:domain <http://schema.mobivoc.org/Action> ;
                    rdfs:range AIRO:Consequence ;
                    rdfs:comment "An action has a consequence." .


#################################################################
#    Data properties
#################################################################

###  http://www.kracr.iiitd.edu.in/ontology/apple/hasScore
apple:hasScore rdf:type owl:DatatypeProperty ;
               owl:propertyDisjointWith time:hasBeginning ,
                                        time:hasEnd .


###  http://www.w3.org/2006/time#hasBeginning
time:hasBeginning rdf:type owl:DatatypeProperty ;
                  rdfs:domain trajectory:TimeEntity ;
                  rdfs:range xsd:dateTime .


###  http://www.w3.org/2006/time#hasEnd
time:hasEnd rdf:type owl:DatatypeProperty ;
            rdfs:domain trajectory:TimeEntity ;
            rdfs:range xsd:dateTimeStamp .


#################################################################
#    Classes
#################################################################

###  http://contextus.net/ontology/ontomedia/core/expression#Context
<http://contextus.net/ontology/ontomedia/core/expression#Context> rdf:type owl:Class ;
                                                                  rdfs:subClassOf [ rdf:type owl:Restriction ;
                                                                                    owl:onProperty apple:describesEvent ;
                                                                                    owl:someValuesFrom <http://schema.org/Event>
                                                                                  ] ;
                                                                  owl:disjointWith apple:Ethics ;
                                                                  rdfs:comment "This class represents the context in which an event or entity exists. (From OC Ontology: https://lov.linkeddata.es/dataset/lov/vocabs/oc)."@en .


###  http://schema.mobivoc.org/Action
<http://schema.mobivoc.org/Action> rdf:type owl:Class ;
                                   rdfs:subClassOf [ rdf:type owl:Restriction ;
                                                     owl:onProperty apple:affects ;
                                                     owl:someValuesFrom apple:PassiveAgent
                                                   ] ,
                                                   [ rdf:type owl:Restriction ;
                                                     owl:onProperty apple:occursInEvent ;
                                                     owl:someValuesFrom <http://schema.org/Event>
                                                   ] ,
                                                   [ rdf:type owl:Restriction ;
                                                     owl:onProperty AIRO:hasConsequence ;
                                                     owl:someValuesFrom AIRO:Consequence
                                                   ] ;
                                   rdfs:comment "Action that is done. (From schema: https://lov.linkeddata.es/dataset/lov/vocabs/mv)"@en .


###  http://schema.org/Event
<http://schema.org/Event> rdf:type owl:Class ;
                          rdfs:subClassOf [ rdf:type owl:Restriction ;
                                            owl:onProperty trajectory:atPlace ;
                                            owl:someValuesFrom trajectory:Place
                                          ] ,
                                          [ rdf:type owl:Restriction ;
                                            owl:onProperty trajectory:atTime ;
                                            owl:someValuesFrom trajectory:TimeEntity
                                          ] ,
                                          [ rdf:type owl:Restriction ;
                                            owl:onProperty apple:occursInDomain ;
                                            owl:someValuesFrom AIRO:Domain
                                          ] ,
                                          [ rdf:type owl:Restriction ;
                                            owl:onProperty participation:hasParticipant ;
                                            owl:someValuesFrom <http://xmlns.com/foaf/0.1/Agent>
                                          ] ,
                                          [ rdf:type owl:Restriction ;
                                            owl:onProperty participation:hasParticipant ;
                                            owl:someValuesFrom [ owl:intersectionOf ( apple:ActiveAgent
                                                                                      apple:PassiveAgent
                                                                                    ) ;
                                                                 rdf:type owl:Class
                                                               ]
                                          ] ;
                          rdfs:comment "An event happening at a certain time and location,> property. Repeated events may be structured as separate Event objects. (From schema: https://lov.linkeddata.es/dataset/lov/vocabs/schema)"@en .


###  http://w3id.org/daselab/onto/trajectory#Place
trajectory:Place rdf:type owl:Class ;
                 rdfs:comment "This is the hook to other pattern/ontology that describes the notion of place, which is more general than just a location/geo-coordinate." ;
                 rdfs:label "Place" .


###  http://w3id.org/daselab/onto/trajectory#TimeEntity
trajectory:TimeEntity rdf:type owl:Class ;
                      rdfs:comment "The hook to class/pattern/ontology that models time, this class provides the temporal extent of the trajectory. One example of time model is the W3C Time Ontology." ;
                      rdfs:seeAlso "http://www.w3.org/2006/time" .


###  http://www.kracr.iiitd.edu.in/ontology/apple/AcademicDomain
apple:AcademicDomain rdf:type owl:Class ;
                     rdfs:subClassOf apple:ProfessionalDomain ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty apple:adheresTo ;
                                       owl:someValuesFrom apple:AcademicEthics
                                     ] ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty apple:adheresTo ;
                                       owl:someValuesFrom apple:ProfessionalEthics
                                     ] ;
                     rdfs:comment "The academic domain" ,
                                  "The domain that relates to the academic area which includes the impact of actions on students, teachers, administration staff, and the research community, along with the ethics of producing and disseminating research, and the conditions in which education is propagated." .


###  http://www.kracr.iiitd.edu.in/ontology/apple/AcademicEthics
apple:AcademicEthics rdf:type owl:Class ;
                     rdfs:subClassOf apple:ProfessionalEthics ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty apple:appliesTo ;
                                       owl:someValuesFrom apple:AcademicDomain
                                     ] ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty apple:appliesTo ;
                                       owl:someValuesFrom apple:ProfessionalDomain
                                     ] ;
                     owl:disjointWith apple:BusinessEthics .


###  http://www.kracr.iiitd.edu.in/ontology/apple/ActiveAgent
apple:ActiveAgent rdf:type owl:Class ;
                  rdfs:subClassOf <http://xmlns.com/foaf/0.1/Agent> ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty apple:hasMoralIntention ;
                                    owl:someValuesFrom apple:MoralIntention
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty traffic:doesAction ;
                                    owl:someValuesFrom <http://schema.mobivoc.org/Action>
                                  ] ;
                  rdfs:comment "An active agent is an agent that does the action in an event." .


###  http://www.kracr.iiitd.edu.in/ontology/apple/AppliedEthics
apple:AppliedEthics rdf:type owl:Class ;
                    rdfs:subClassOf apple:Ethics ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty apple:appliesPhilosophy ;
                                      owl:someValuesFrom apple:AppliedEthicsPhilosophy
                                    ] ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty apple:appliesTo ;
                                      owl:someValuesFrom AIRO:Domain
                                    ] ;
                    rdfs:comment """Applied ethics is a sub-field of ethics, amongst the three major fields of the subject. It has to do with the application of ethical philosophies in various domains. 

Bioethics, for example, governs the applications of ethics in the medical domain. Similarly, professional ethics describes how ethical theories may be applied to the academic or corporate domains.""" .


###  http://www.kracr.iiitd.edu.in/ontology/apple/AppliedEthicsPhilosophy
apple:AppliedEthicsPhilosophy rdf:type owl:Class ;
                              rdfs:comment "A philosophy that acts as a guiding principle to make ethical decisions pertaining to certain real-world domains."@en .


###  http://www.kracr.iiitd.edu.in/ontology/apple/BioethicsIssue
apple:BioethicsIssue rdf:type owl:Class ;
                     rdfs:subClassOf apple:EthicalIssue ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty apple:resolvedBy ;
                                       owl:someValuesFrom apple:BioethicsPhilosophy
                                     ] ;
                     rdfs:comment "A bioethics issue is an ethical issue that occurs in the medical domain." .


###  http://www.kracr.iiitd.edu.in/ontology/apple/BioethicsPhilosophy
apple:BioethicsPhilosophy rdf:type owl:Class ;
                          rdfs:subClassOf apple:AppliedEthicsPhilosophy ;
                          rdfs:comment "A philosophy that acts as a guiding principle to make ethical decisions pertaining to bioethics in a real-world medical domain."@en .


###  http://www.kracr.iiitd.edu.in/ontology/apple/BusinessEthicalIssue
apple:BusinessEthicalIssue rdf:type owl:Class ;
                           rdfs:subClassOf apple:EthicalIssue ,
                                           [ rdf:type owl:Restriction ;
                                             owl:onProperty apple:resolvedBy ;
                                             owl:someValuesFrom apple:BusinessEthicsPhilosophy
                                           ] ;
                           rdfs:comment "A business ethical issue is an ethical issue that occurs in the professional domain." .


###  http://www.kracr.iiitd.edu.in/ontology/apple/BusinessEthics
apple:BusinessEthics rdf:type owl:Class ;
                     rdfs:subClassOf apple:ProfessionalEthics ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty apple:appliesTo ;
                                       owl:someValuesFrom apple:CorporateDomain
                                     ] ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty apple:appliesTo ;
                                       owl:someValuesFrom apple:ProfessionalDomain
                                     ] .


###  http://www.kracr.iiitd.edu.in/ontology/apple/BusinessEthicsPhilosophy
apple:BusinessEthicsPhilosophy rdf:type owl:Class ;
                               rdfs:subClassOf apple:AppliedEthicsPhilosophy ;
                               rdfs:comment "A philosophy that acts as a guiding principle to make ethical decisions pertaining to business ethics in a real-world professional domain."@en .


###  http://www.kracr.iiitd.edu.in/ontology/apple/CharacteristicOfConsequence
apple:CharacteristicOfConsequence rdf:type owl:Class ;
                                  rdfs:comment "The characteristic of a consequence describes the impact of an action from the perspective of its duration, severity, and utility. The consequences of an action can have certain utilitarian and temporal aspects which may be defined using these characteristics."@en .


###  http://www.kracr.iiitd.edu.in/ontology/apple/Consequentialism
apple:Consequentialism rdf:type owl:Class ;
                       rdfs:subClassOf apple:NormativeEthics ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty apple:associatedWith ;
                                         owl:someValuesFrom AIRO:Consequence
                                       ] ;
                       rdfs:comment "Consequentialism is an ethical theory that judges whether or not something is right by what its consequences are. For instance, most people would agree that lying is wrong. But if telling a lie would help save a person's life, consequentialism says it's the right thing to do."@en .


###  http://www.kracr.iiitd.edu.in/ontology/apple/ConsumerCentricBusinessEthicalIssue
apple:ConsumerCentricBusinessEthicalIssue rdf:type owl:Class ;
                                          rdfs:subClassOf apple:BusinessEthicalIssue ;
                                          rdfs:comment "A consumer-centric ethical issue is a business ethical issue that discusses the consequences of an action on one or more consumer(s)." .


###  http://www.kracr.iiitd.edu.in/ontology/apple/CorporateDomain
apple:CorporateDomain rdf:type owl:Class ;
                      rdfs:subClassOf apple:ProfessionalDomain ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty apple:adheresTo ;
                                        owl:someValuesFrom apple:BusinessEthics
                                      ] ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty apple:adheresTo ;
                                        owl:someValuesFrom apple:ProfessionalEthics
                                      ] .


###  http://www.kracr.iiitd.edu.in/ontology/apple/Deontology
apple:Deontology rdf:type owl:Class ;
                 rdfs:subClassOf apple:NormativeEthics ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty apple:associatedWith ;
                                   owl:someValuesFrom apple:EthicalPrinciple
                                 ] ;
                 rdfs:comment "Deontology is a topic under normative ethics that takes from Kantianism and has to do with what is right and what is wrong. If an action is intrinsically wrong then the committing of such an action would also be wrong or unethical according to this ethical theory." .


###  http://www.kracr.iiitd.edu.in/ontology/apple/DurationOfConsequence
apple:DurationOfConsequence rdf:type owl:Class ;
                            rdfs:subClassOf apple:CharacteristicOfConsequence ,
                                            [ rdf:type owl:Restriction ;
                                              owl:onProperty apple:hasScore ;
                                              owl:minCardinality "1"^^xsd:nonNegativeInteger
                                            ] ;
                            rdfs:comment "The duration of a consequence could be either short-term or long-term and this aspect plays a role in the morality of an action. From a consequentialist viewpoint, if an action leads to a bad consequence as well as a good consequence, a temporal assessment can give insight into whether it is morally right or wrong. If the action leads to a bad short-term consequence but a good long-term consequence then it may be an overall morally right action."@en .


